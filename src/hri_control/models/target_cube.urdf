<?xml version="1.0"?>
<robot name="target_cube">

  <!-- WORLD LINK REQUIRED FOR FIXED JOINT -->
  <link name="world"/>

  <link name="cube">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
      <material name="green">
        <color rgba="0 1 0 1"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="world_to_cube" type="fixed">
    <parent link="world"/>
    <child link="cube"/>
    <origin xyz="0 0 0"/>
  </joint>

</robot>

